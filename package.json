{
    "name": "it-office",
    "description": "IT Mini Game For Github Pages",
    "version": "1.0.0",
    "keywords": [
        "phaser",
        "phaser3",
        "typescript",
        "vue",
        "vite"
    ],
    "scripts": {
        "dev": "node log.js dev & vite --config vite/config.dev.mjs",
        "build": "node log.js build & vite build --config vite/config.prod.mjs",
        "dev-nolog": "vite --config vite/config.dev.mjs",
        "build-nolog": "vite build --config vite/config.prod.mjs",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "phaser": "^3.90.0",
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@types/node": "^20.11.20",
        "@vitejs/plugin-vue": "^5.2.2",
        "@vue/tsconfig": "^0.7.0",
        "prettier": "^3.6.2",
        "terser": "^5.31.0",
        "typescript": "~5.7.2",
        "vite": "^6.3.1",
        "vue-tsc": "^2.2.8"
    }
}
